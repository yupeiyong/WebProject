        [TestMethod]
        public void TestClientPostion()
        {

            DateTime dt = new DateTime(2016, 12, 23, 13, 50, 50);
            IProtocolBuffer buffer = MessageFactory.MessateToBuffer<Messages.ClientPostion>(BUSINESS_NO, SIM, (m, b) =>
            {
                b.Direction = 4;
